@startuml

node PC_Client{
 component Interface_Utilisateur{ 
           folder EntreeCdeUI.exe         
          
 }          
}

cloud AWS {
  node Serveur{
                component SystemeCommande
                component InventaireStock
                component Comptabilite
                component LogisticCommande
                component InterfaceDB
  }
}

